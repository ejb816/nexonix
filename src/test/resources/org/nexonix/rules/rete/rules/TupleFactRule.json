{
  "typeName": {
    "name": "TupleFactRule",
    "namePackage": ["org", "nexonix", "rules", "rete", "rules"]
  },
  "variables": [
    {
      "kind": "Variable",
      "name": "fact",
      "valueType": "(Int,Int,Int)"
    }
  ],
  "conditions": [
    {
      "kind": "Condition",
      "value": "fact._1.equals(1) && fact._2.equals(2) && fact._3.equals(3)",
      "parameters": [
        {"kind": "Parameter", "name": "fact", "valueType": "(Int,Int,Int)"}
      ]
    }
  ],
  "action": {
    "kind": "Action",
    "body": [
      {
        "kind": "Monadic",
        "value": "println(fact)"
      },
      {
        "kind": "Monadic",
        "value": "println((fact._1, fact._2, fact._3))"
      }
    ]
  }
}
