{
  "name": "RemoveFromSequence",
  "variables": {
    "fp": "FindPrimes",
    "i1": "Int",
    "i2": "Int",
    "i3": "Int"
  },
  "conditions": [
    "i1 * i2 == i3"
  ],
  "values": {},
  "action": [
    "val baseCounter = fp.conditionalPrint(i3, s\" -> $i1 * $i2 = $i3\")",
    "context.delete(i3)",
    "context.delete(fp)",
    "context.insert(FindPrimes(fp.maximum, fp.delta, baseCounter._1, baseCounter._2))"
  ]
}
